{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": true,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["src/.umi/*"],
      "@utils/*": ["src/utils/*"],
      "@components/*": ["src/components/*"]
    },
    "typeRoots": ["node_modules/@types", "node_modules/@typed-mxgraph"],
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules", "lib", "es", "dist", "typings", "**/__test__", "test"]
}
